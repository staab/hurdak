{
  "name": "hurdak",
  "version": "0.2.8",
  "private": false,
  "description": "A handy utility library to complement ramda.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/staab/hurdak.git"
  },
  "keywords": [
    "utility"
  ],
  "type": "module",
  "author": "Jon Staab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/staab/hurdak/issues"
  },
  "homepage": "https://github.com/staab/hurdak",
  "types": "./build/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/src/index.d.ts",
      "import": "./build/src/index.mjs",
      "require": "./build/src/index.cjs"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "pub": "npm i && npm run lint && tsc-multi && npm publish",
    "build": "tsc-multi",
    "clean": "gts clean",
    "test": "NODE_ENV=test mocha --colors ./test/**/*.test.js",
    "lint": "gts lint",
    "fix": "gts fix",
    "prepare": "yarn run build",
    "pretest": "yarn run build",
    "posttest": "yarn run lint"
  },
  "devDependencies": {
    "@types/node": "20.4.6",
    "@types/ramda": "^0.29.8",
    "chai": "^4.3.10",
    "gts": "^5.0.1",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "tsc-multi": "^1.1.0",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "ramda": "^0.29.1"
  }
}
